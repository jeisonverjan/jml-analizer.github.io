<div class="home-box">
  <div class="home-box-title">
    <h1>Analyze your JML file</h1>
    <p>Download the ToDo table from Service Now and upload it.</p>
  </div>
  <div class="home-box-form">
    <form action="" class="home-form" method="POST">
      <div class="home-form-select-box">
        <select name="" id="" class="home-form-select">
          <option value="">Select an Option</option>
          <option value="">Peru</option>
          <option value="">Ecuador</option>
          <option value="">Brazil</option>
        </select>
        <div class="form-select-icon">
          <i class="fa-solid fa-caret-down"></i>
        </div>
      </div>
      <div class="home-form-file">
        <input type="file" name="" id="file" required/>
        <label for="file">
          <i class="fa-solid fa-folder-open"> </i>
          &nbsp; Choose a File
        </label>
      </div>  
      <div class="home-form-btn">
        <button>Analyze</button>
      </div>
    </form>
  </div>
</div>

{{> population/populationRender}}
